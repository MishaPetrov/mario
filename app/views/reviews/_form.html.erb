<%= render('layouts/errors', :object => @review) %>

<%= form_for [@product, @review] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :author %>
  <%= f.label :review %>
  <%= f.text_field :content_body %>
  <%= f.label :rating %>
  <%= f.range_field(:rating, in: 1..5) %>
  <p><%= f.submit %></p>
<% end %>
